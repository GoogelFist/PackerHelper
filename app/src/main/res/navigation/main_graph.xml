<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/main_graph"
        app:startDestination="@id/palletFragment">
    <fragment
            android:id="@+id/palletFragment"
            android:name="com.github.googelfist.packerhelper.presentation.screens.pallet.PalletFragment"
            android:label="PalletFragment" />
    <fragment
            android:id="@+id/piecesTabsFragment"
            android:name="com.github.googelfist.packerhelper.presentation.PiecesTabsFragment"
            android:label="PiecesTabsFragment" />
    <fragment
            android:id="@+id/tabsFragment"
            android:name="com.github.googelfist.packerhelper.presentation.TabsFragment"
            android:label="TabsFragment" >
        <action
                android:id="@+id/action_tabsFragment_to_palletFragment2"
                app:destination="@id/palletFragment" />
        <action
                android:id="@+id/action_tabsFragment_to_piecesTabsFragment"
                app:destination="@id/piecesTabsFragment" />
    </fragment>
</navigation>